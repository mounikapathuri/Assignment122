package assi12;

public class result {

	public static void main(String[] args) {
		
		int a=20;
		int b=10;
		int c,d,e;
		System.out.println(""+a);
		 b=a-- - --a;
			System.out.println("a-- - --a="+b);
		 c=a--;
			System.out.println("a-- ="+c);
		 d=a>>2;
			System.out.println("a>>="+d);
		 e=a&b;
		System.out.println("a&b ="+e);
		

	}

}
